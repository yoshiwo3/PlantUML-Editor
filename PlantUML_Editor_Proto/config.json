{
  "performance": {
    "measurementEnabled": true,
    "optimizationEnabled": true,
    "autoOptimizationEnabled": true,
    "thresholds": {
      "parseTime": 100,
      "renderTime": 200,
      "syncTime": 50,
      "maxMemoryUsage": 150,
      "maxRenderTime": 500,
      "minCacheHitRate": 0.8,
      "maxInputLength": 10000
    },
    "webWorkers": {
      "enabled": true,
      "maxWorkers": 4,
      "timeout": 10000
    },
    "cache": {
      "enabled": true,
      "maxCacheSize": 100,
      "defaultTTL": 3600000,
      "compression": {
        "enabled": true,
        "threshold": 1000
      }
    },
    "batchProcessing": {
      "enabled": true,
      "batchSize": 5,
      "batchDelay": 100
    },
    "virtualScroll": {
      "enabled": true,
      "itemHeight": 50,
      "visibleBuffer": 5
    },
    "monitoring": {
      "intervalMs": 5000,
      "memoryCheckIntervalMs": 10000,
      "performanceHistorySize": 100
    },
    "optimization": {
      "strategies": {
        "lazyLoading": {
          "enabled": true,
          "priority": 1
        },
        "memoryOptimization": {
          "enabled": true,
          "priority": 2,
          "aggressiveCleanup": false
        },
        "renderOptimization": {
          "enabled": true,
          "priority": 3,
          "virtualization": true,
          "batchRendering": true
        },
        "cacheOptimization": {
          "enabled": true,
          "priority": 4,
          "prefetching": true,
          "compression": true
        }
      }
    },
    "alerts": {
      "enabled": true,
      "highMemoryUsageAlert": true,
      "slowRenderAlert": true,
      "lowCacheHitRateAlert": true,
      "console": {
        "logLevel": "warn",
        "detailedMetrics": false
      }
    }
  },
  "features": {
    "realtimeSync": {
      "enabled": true,
      "debounceDelay": 300,
      "maxSyncFrequency": 1000
    },
    "validation": {
      "enabled": true,
      "realtime": true,
      "background": true
    },
    "ast": {
      "enabled": true,
      "cacheEnabled": true,
      "optimizeStructure": true
    },
    "gui": {
      "enabled": true,
      "lazyRendering": true,
      "virtualScrolling": true
    }
  },
  "debug": {
    "enabled": false,
    "verbose": false,
    "performanceLogging": true,
    "metricsDisplay": false
  },
  "experimental": {
    "aiOptimization": false,
    "predictiveOptimization": false,
    "advancedCaching": true,
    "parallelProcessing": true
  }
}